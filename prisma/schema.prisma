
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  telegramId String  @unique
  name      String
  phone     String
  campus    String
  restaurant String?
  createdAt DateTime @default(now())
}
